.listsc {
	width: 100%;
}

.z_photo {
	width: 100%;
	height: 100%;
}

.z_photo section {
	position: relative;
	background-image: url('http://www.fengliao8.com/images/ayzmb.png');
	background-repeat: no-repeat;
	background-position: center 25%;
	background-size: cover;
	float: left;
	margin-right: 43px;
	margin-bottom: 30px;
	border-radius: 10px;
}

.z_photo div {
	position: relative;
	background-image: url('http://www.fengliao8.com/images/ayzmb.png');
	background-repeat: no-repeat;
	background-position: center 25%;
	background-size: cover;
	float: left;
	margin-right: 43px;
	margin-bottom: 30px;
	border-radius: 10px;
}

.z_photo div img {
	width: 100%;
	height: 100%
}

.up-section .close-upimg {
	position: absolute;
	top: 6px;
	right: 8px;
	display: none;
	z-index: 10;
}

.z_photo section:nth-of-type(3n) {
	margin: 0;
	margin-bottom: 30px;
}

.z_photo section img {
	width: 100%;
	height: 100%;
	display: block;
}

.z_photo section p {
	display: none;
}

.z_photo section img.close-upimg {
	width: 20px;
	height: 20px;
	text-align: center;
	position: absolute;
	right: 3px;
	top: 3px;
	display: none;
	z-index: 666;
}

