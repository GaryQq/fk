}

.listsc>div .delte img {
	width: 16px;
	height: 16px;
	display: inline-block;
	margin-top: 17px;
}

.listsc>div input {
	width: 100%;
	height: 100%;
	display: block;
	position: absolute;
	left: 0;
	top: 0;
	opacity: 0;
	z-index: 5;
}

.mask-content {
	width: 500px;
	height: 300px;
	background-color: #EDEDED;
	z-index: 669;
	position: fixed;
	left: 50%;
	margin-left: -250px;
	top: 50%;
	margin-top: -150px;
	text-align: center;
	color: #333333;
	border-radius: 8px;
	display: none;
}

.del-p {
	font-weight: bold;
	font-size: 30px;
	margin-top: 95px;
}

.check-p {
	display: flex;
	width: 100%;
	margin-top: 81px;
}

.check-p span {
	flex: 1;
	text-align: center;
	font-size: 26px;
	line-height: 80px;
}

.posiblock {
	background-color: rgba(0, 0, 0, .7);
	width: 100%;
	height: 100%;
	position: fixed;
	left: 0;
	top: 0;
	z-index: 666;
}

.imga {
	width: 60px;
	height: 60px;
	position: absolute;
	left: 50%;
	margin-left: -30px;
	top: 50%;
	margin-top: -30px;
}

.up-section:hover {
	border: 2px solid #f15134;
}

.up-section:hover .close-upimg {
	display: block;
}